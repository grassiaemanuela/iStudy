<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
            xmlns:app="http://schemas.android.com/apk/res-auto"
            xmlns:tools="http://schemas.android.com/tools" android:id="@+id/startfragment"
            app:startDestination="@id/startFragment">

    <fragment android:id="@+id/startFragment" android:name="com.example.istudy.StartFragment"
              android:label="fragment_start" tools:layout="@layout/fragment_start">
        <action android:id="@+id/action_startFragment_to_edificioFragment" app:destination="@id/edificioFragment"/>
    </fragment>
    <fragment android:id="@+id/edificioFragment" android:name="com.example.istudy.EdificioFragment"
              android:label="fragment_edificio" tools:layout="@layout/fragment_edificio">
        <action android:id="@+id/action_edificioFragment_to_viaRomaFragment" app:destination="@id/viaRomaFragment"/>
    </fragment>
    <fragment android:id="@+id/viaRomaFragment" android:name="com.example.istudy.ViaRomaFragment"
              android:label="fragment_via_roma" tools:layout="@layout/fragment_via_roma"/>
</navigation>